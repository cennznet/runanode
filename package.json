{
  "name": "rUN",
  "version": "0.11.0",
  "description": "rUN digital wallet power by Centrality.ai",
  "homepage": "https://bitbucket.org/centralitydev/cennz-node-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/centralitydev/cennz-node-ui.git"
  },
  "license": "ISC",
  "author": "Centrality <developers@centrality.ai>",
  "main": "./app/main.prod.js",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
    "build-preload": "cross-env webpack --config ./configs/webpack.config.preload.babel.js --colors",
    "build-preload-dev": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.preload.babel.js --colors",
    "build-e2e": "cross-env E2E_BUILD=true yarn build",
    "build-main": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.renderer.prod.babel.js --colors",
    "dev": "yarn build-preload-dev && cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "clean": "rimraf ./release -s",
    "cucumber": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 LAUNCHER_CONFIG=./launcher-config.yaml cucumber-js --compiler js:@babel/register -f node_modules/cucumber-pretty --format-options '{\"snippetInterface\": \"async-await\"}' --tags 'not @skip'",
    "cucumber:rebuild": "yarn build && yarn cucumber",
    "cucumber:fast": "npm run cucumber -- --tags 'not @slow'",
    "cucumber:watch": "npm run cucumber -- --tags @watch",
    "cucumber:watch:debug": "KEEP_APP_AFTER_TESTS=true npm run cucumber -- --tags @watch",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "preinstall": "node ./internals/scripts/CheckYarn.js",
    "postinstall": "yarn flow-typed && electron-builder install-app-deps package.json && yarn build-dll",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{*{js,jsx,json},babelrc,eslintrc,prettierrc,stylelintrc}'",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix; exit 0",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{css,scss}'",
    "package": "yarn clean && yarn build && electron-builder build --publish never",
    "package-all": "yarn clean && yarn build && electron-builder build -mwl --publish never",
    "package-ci": "yarn clean && yarn postinstall && yarn build && electron-builder --publish always",
    "package-linux": "yarn clean && yarn build && electron-builder build -l --publish never",
    "package-win": "yarn clean && yarn build && electron-builder -w --publish never --config.compression=store --config.publish.provider=github --config.publish.owner=cennznet --config.publish.repo=cennz-node-release --config.publish.private=true --config.publish.releaseType=release",
    "prestart": "yarn build",
    "release": "yarn clean && yarn build && electron-builder -mwl --publish never",
    "release:mac": "yarn clean && yarn build && electron-builder -m --publish never",
    "release:github": "yarn clean && yarn build && electron-builder -mwl --publish always --config.compression=store --config.publish.provider=github --config.publish.owner=cennznet --config.publish.repo=cennz-node-release --config.publish.private=true --config.publish.releaseType=release",
    "release:minio": "yarn clean && yarn build && electron-builder -m --publish always --config.compression=store --config.publish.provider=s3 --config.publish.endpoint=http://localhost:9000 --config.publish.bucket=auto-update",
    "release:minio-all": "yarn clean && yarn build && electron-builder -mlw --publish always --config.compression=store --config.publish.provider=s3 --config.publish.endpoint=http://localhost:9000 --config.publish.bucket=auto-update",
    "start": "yarn build-preload && cross-env NODE_ENV=production LAUNCHER_CONFIG=./launcher-config.yaml electron ./app/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./app/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development LAUNCHER_CONFIG=./launcher-config.yaml webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js",
    "styleguide": "styleguidist server",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 jest",
    "test-all": "yarn lint && yarn flow && yarn build && yarn test && yarn cucumber",
    "test-e2e": "node -r @babel/register ./internals/scripts/CheckBuiltsExist.js && cross-env LAUNCHER_CONFIG=./launcher-config.yaml NODE_ENV=test testcafe electron:./ ./test/e2e/HomePage.e2e.js",
    "test-e2e-live": "node -r @babel/register ./internals/scripts/CheckBuiltsExist.js && cross-env NODE_ENV=test testcafe-live electron:./ ./test/e2e/HomePage.e2e.js",
    "test-watch": "yarn test --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ],
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
