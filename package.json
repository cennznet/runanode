{
  "name": "cennz-node-ui",
  "version": "0.0.3",
  "description": "CENNZ node Electron App",
  "main": "./app/main/main.prod.js",
  "scripts": {
    "build": "concurrently \"yarn build-main\" \"yarn build-renderer\"",
    "build-main": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.main.prod.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.renderer.prod.js",
    "clean": "rimraf ./release -s",
    "dev": "cross-env START_HOT=1 yarn start-renderer-dev",
    "eslint": "eslint --ext js --ext jsx app",
    "release": "yarn clean && yarn build && electron-builder -mwl --publish never",
    "release:github": "yarn clean && yarn build && electron-builder -mwl --publish always --config.compression=store --config.publish.provider=github --config.publish.owner=cennznet --config.publish.repo=cennz-node-release --config.publish.private=true --config.publish.releaseType=release",
    "release:minio": "yarn clean && yarn build && electron-builder -mwl --publish always --config.compression=store --config.publish.provider=s3 --config.publish.endpoint=http://10.9.30.50:9000 --config.publish.bucket=test-update",
    "prestart": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "start": "cross-env NODE_ENV=production electron ./app/main/main.prod.js",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r @babel/register ./app/main/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.config.renderer.dev.js",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:watch": "yarn test --watch"
  },
